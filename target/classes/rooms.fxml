<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="rentui.Rooms"
      prefHeight="300.0" prefWidth="300.0" fx:id="roomsPane">
    <HBox>
        <Button onAction="#addRoom" text="Add Room"/>
        <Button onAction="#removeRoom" text="Remove Room"/>
        <Button onAction="#editRoom" text="Edit Room"/>
    </HBox>
    <TableView fx:id="roomsTable" onKeyPressed="#onKeyPressed"/>
</VBox>


