<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="rentui.Bills"
      prefHeight="300.0" prefWidth="200.0" fx:id="billsPane">
    <HBox>
        <Button onAction="#addBill" text="Add Bill"/>
        <Button onAction="#removeBill" text="Remove Bill"/>
        <Button onAction="#editBill" text="Edit Bill"/>
    </HBox>
    <TableView fx:id="billsTable" onKeyPressed="#onKeyPressed"/>


</VBox>


